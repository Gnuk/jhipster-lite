{
  "name": "jhlite",
  "version": "0.20.1-SNAPSHOT",
  "description": "JHipster Lite",
  "homepage": "https://www.jhipster.tech/",
  "bugs": "https://github.com/jhipster/jhipster-lite/issues",
  "repository": {
    "type": "git",
    "url": "git://github.com/jhipster/jhipster-lite.git"
  },
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/generator-jhipster"
  },
  "license": "Apache-2.0",
  "scripts": {
    "build": "vue-tsc -p tsconfig.build.json --noEmit && vite build --emptyOutDir",
    "dev": "npm run webapp:serve",
    "format": "prettier . --write",
    "lint": "npm run lint:js && npm run prettier:format",
    "lint:ci": "npm run lint:ci:js && npm run prettier:check",
    "lint:ci:js": "npm run lint:js:run -- --no-fix",
    "lint:js": "npm run lint:js:run -- --fix",
    "lint:js:run": "eslint --ext .js,.ts,.vue --ignore-path .gitignore  src/main/webapp src/test/javascript",
    "lint:pug": "npm run lint:ci:pug",
    "lint:sass": "npm run lint:ci:sass -- --fix",
    "prettier:check": "npm run prettier:run -- --check",
    "prettier:format": "npm run prettier:run -- --write",
    "prettier:run": "prettier \"{{src/**/,}*.{md,json,yml,html,vue,java,xml},*.{js,ts},src/{main/glyph,main/webapp,test/javascript}/**/*.{css,scss}}\"",
    "preview": "vite preview",
    "start": "npm run dev",
    "test": "npm run vitest -- run --coverage --",
    "test:watch": "npm run vitest --",
    "vitest": "vitest",
    "webapp:serve": "vite"
  },
  "dependencies": {
    "axios": "1.1.3",
    "mitt": "3.0.0",
    "vue": "3.2.41",
    "vue-router": "4.1.6"
  },
  "devDependencies": {
    "@prettier/plugin-xml": "2.2.0",
    "@rushstack/eslint-patch": "1.2.0",
    "@types/sinon": "10.0.13",
    "@typescript-eslint/parser": "5.41.0",
    "@vitejs/plugin-vue": "3.1.2",
    "@vitest/coverage-istanbul": "0.24.3",
    "@vue/eslint-config-prettier": "7.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "2.2.0",
    "eslint": "8.26.0",
    "eslint-plugin-vue": "8.7.1",
    "jsdom": "20.0.1",
    "lint-staged": "13.0.3",
    "postcss-scss": "4.0.5",
    "prettier": "2.7.1",
    "prettier-plugin-java": "1.6.2",
    "prettier-plugin-packagejson": "2.3.0",
    "pug-lint": "2.6.0",
    "rimraf": "3.0.2",
    "sass": "1.55.0",
    "sinon": "14.0.1",
    "typescript": "4.8.4",
    "vite": "3.1.8",
    "vitest": "0.24.3",
    "vue-tsc": "1.0.9"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "cacheDirectories": [
    "node_modules"
  ],
  "jestSonar": {
    "reportPath": "target/test-results",
    "reportFile": "TESTS-results-sonar.xml"
  }
}
